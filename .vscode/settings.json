{
  "tailwindCSS.includeLanguages": {
    "liquid": "html"
  },
  "files.associations": {
    "*.liquid": "html"
  },
  "[json]": {
    "editor.indentSize": 2,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 1
  },
  "[js]": {
    "editor.indentSize": 2,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 1
  },
  "[html]": {
    "editor.indentSize": 2,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 1
  },
  "themeCheck.checkOnSave": true
}
